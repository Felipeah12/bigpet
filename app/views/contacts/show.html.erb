<div class="dashboard-content">	
  <div class="row align-items-center mt-devices">
    <h3 class="admintitle">Contacto</h3>
  </div>

  <div class="row">
    <div class="col-sm-12 col-md-8 offset-md-2 bg-light-custom border-irregular1 block-padding p30">
      <div class="col-lg-12">              	
        <div class="row">
          <div class="col-md-12">
            <strong><%= t('activerecord.attributes.contact.name') %>: </strong> <%= @contact.name %> 
          </div>
          <div class="col-md-12">
            <strong><%= t('activerecord.attributes.contact.email') %>: </strong> <%= @contact.email %>
          </div>
          <div class="col-md-12">
            <strong><%= t('activerecord.attributes.contact.subject') %>: </strong> <%= @contact.subject %>
          </div>
          <div class="col-md-12 mt10">
            <strong><%= t('activerecord.attributes.contact.message') %>: </strong> <%= @contact.message %>
          </div>
        </div>
      </div>

      <div class="col-sm-12">
        <%= link_to t('btn.back'), contacts_path, class: 'btn btn-primary pull-right btn-sm', method: :get %>
      </div>
    </div>
  </div>
</div>
